{
    "name": "nahid/qarray",
    "description": "QArray is a PHP abstraction for quering array",
    "keywords": ["array", "query", "php"],
    "homepage": "https://github.com/nahid/qarray",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        }
    ],
  "require": {
    "php": ">=5.6.0",
        "myclabs/deep-copy": "^1.8"
  },
  "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "symfony/var-dumper": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Nahid\\QArray\\": "src/"
    },
    "files":[
      "helpers/qarray.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nahid\\JsonQ\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
